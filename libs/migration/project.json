{
  "name": "migration",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/migration/src",
  "targets": {
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/target"
      ],
      "options": {
        "command": "cargo check --package migration",
        "cwd": "{workspaceRoot}"
      }
    },
    "test": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/target"
      ],
      "options": {
        "command": "cargo test --package migration",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo clippy --package migration -- -D warnings",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tags": []
}
