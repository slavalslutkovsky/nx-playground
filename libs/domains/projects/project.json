{
  "name": "domain_projects",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/domains/projects/src",
  "targets": {
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/target"
      ],
      "options": {
        "command": "cargo check --package domain_projects",
        "cwd": "{workspaceRoot}"
      }
    },
    "test": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/target"
      ],
      "options": {
        "command": "cargo test --package domain_projects",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo clippy --package domain_projects -- -D warnings",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tags": []
}
