{
  "name": "domain_cloud_resources",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/domains/cloud_resources/src",
  "targets": {
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/target"],
      "options": {
        "command": "cargo check --package domain_cloud_resources",
        "cwd": "{workspaceRoot}"
      }
    },
    "test": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/target"],
      "options": {
        "command": "cargo test --package domain_cloud_resources",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo clippy --package domain_cloud_resources",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tags": []
}
