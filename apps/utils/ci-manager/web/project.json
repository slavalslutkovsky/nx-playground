{
  "name": "ci-manager-web",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/utils/ci-manager/web/src",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "apps/utils/ci-manager/web"
      }
    },
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "vite build",
        "cwd": "apps/utils/ci-manager/web"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "apps/utils/ci-manager/web"
      }
    },
    "lint": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check .",
        "cwd": "apps/utils/ci-manager/web"
      }
    }
  },
  "tags": []
}
