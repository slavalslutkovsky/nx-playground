{
  "name": "zerg-web",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/zerg/web/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite serve",
        "cwd": "apps/zerg/web"
      }
    },
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build",
        "cwd": "apps/zerg/web"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "apps/zerg/web"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check --write {projectRoot}"
      }
    }
  },
  "tags": []
}
