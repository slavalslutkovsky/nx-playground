{
  "name": "zerg_api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/zerg/api/src",
  "targets": {
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/target"
      ],
      "options": {
        "command": "cargo build --package zerg_api",
        "cwd": "{workspaceRoot}"
      },
      "configurations": {
        "production": {
          "command": "cargo build --package zerg_api --release"
        }
      }
    },
    "test": {
      "cache": true,
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/target"
      ],
      "options": {
        "command": "cargo test --package zerg_api",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo clippy --package zerg_api",
        "cwd": "{workspaceRoot}"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo run --package zerg_api",
        "cwd": "{workspaceRoot}"
      },
      "configurations": {
        "production": {
          "command": "cargo run --package zerg_api --release"
        }
      }
    }
  },
  "tags": []
}
